{
  "kind": "collectionType",
  "collectionName": "applications",
  "info": {
    "singularName": "application",
    "pluralName": "applications",
    "displayName": "Application",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "applicants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "applications"
    },
    "jobName": {
      "type": "string",
      "required": true
    },
    "jobSalary": {
      "type": "biginteger",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "accepted",
        "rejected"
      ],
      "default": "pending"
    },
    "jobs": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::job.job",
      "inversedBy": "applications"
    }
  }
}
